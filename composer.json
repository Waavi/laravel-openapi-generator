{
    "name": "waavi/laravel-openapi-generator",
    "description": "OpenApi generator",
    "require": {
        "illuminate/support": "5.*",
        "symfony/yaml": "^4.3",
        "nikic/php-parser": "^4.0|^4.1|^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Waavi",
            "email": "info@waavi.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Waavi\\LaravelOpenApiGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waavi\\LaravelOpenApiGenerator\\OpenApiGeneratorServiceProvider"
            ]
        }
    }
}
